{
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "all",
  "proseWrap": "never",
  "overrides": [{ "files": ".prettierrc", "options": { "parser": "json" } }],
  "plugins": [
    "prettier-plugin-packagejson",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "react",
    "^(?!react)\\w+$",
    "antd/(.*)",
    "<THIRD_PARTY_MODULES> ",
    "@/(.*)",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true
}
